                },
                {
                    "definition": {
                        "activation": {
                            "activationTime": null,
                            "activationType": null
                        },
                        "description": "<p>You gain proficiency with the artisan&rsquo;s tools of your choice: smith&rsquo;s tools, brewer&rsquo;s supplies, or mason&rsquo;s tools.</p>",
                        "displayOrder": 9,
                        "entityTypeId": 1960452172,
